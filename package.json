{
  "name": "node_server_template",
  "version": "1.0.0",
  "description": "template of node_server using express.js",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --allowJs",
    "start": "node ./dist/index.js",
